{
  "extends": [
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "ignorePatterns": ["/*.config.js"],
  "rules": {
    "newline-before-return": "error",
    "no-console": "error",
    "prefer-template": "error",
    "func-style": ["error", "expression"]
  }
}
